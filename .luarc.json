{
    "runtime.version": "LuaJIT",
    "diagnostics.globals": [
        "__handleEvent",
        "__enableEvent",
        "__disableEvent",
        "__tick",
        "__createHook",
        "__removeHook"
    ],
    "diagnostics.disable": [
        "unused-function",
        "undefined-field",
        "param-type-mismatch",
        "lowercase-global"
    ]
}